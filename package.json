{
  "name": "gauge-progress",
  "description": "svg gauge progress",
  "keywords": [
    "svg",
    "gauge",
    "progress",
    "browserify"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jameskyburz/gauge-progress.git"
  },
  "author": {
    "name": "James Kyburz",
    "email": "james.kyburz@gmail.com"
  },
  "browserify": {
    "transform": [
      "hyperxify"
    ]
  },
  "main": "index.js",
  "version": "2.1.5",
  "scripts": {
    "test": "zuul --no-coverage test/*.js --phantom | faucet",
    "test-local": "zuul --no-coverage test/*.js --local --open",
    "test-browsers": "remove-saucelabs-jobs-by-build && zuul --no-coverage test/*.js",
    "prepublish": "standard"
  },
  "dependencies": {
    "hyperx": "1.3.1",
    "hyperxify": "1.2.1",
    "main-loop": "3.2.0",
    "virtual-dom": "2.1.1",
    "xtend": "4.0.1"
  },
  "devDependencies": {
    "faucet": "0.0.1",
    "function-bind": "1.1.0",
    "remove-saucelabs-jobs-by-build": "1.0.9",
    "standard": "7.1.0",
    "tape": "4.5.1",
    "zuul": "3.10.1"
  },
  "license": "MIT"
}
